			{
			},
		},
		["caption_info"] = {
			["align"] = "bottom",
			["b_use_mask"] = true,
			["text"] = "[size 17]米达斯盯着橙色的火焰，视线渐渐放空。",
			["text_align"] = "center",
		},
		["characters"] = {
			{
				["anim"] = "normal2",
