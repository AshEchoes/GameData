		["caption_info"] = {
			["is_show_all"] = true,
			["mask_alpha"] = 0.60000002384186,
			["text"] = "[size 17][c eae3d6][font cusong]1105年12月2日           天气晴[/c][/font]\n\n[size 16]这个家没有衡哥得散。",
