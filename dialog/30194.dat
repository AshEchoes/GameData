				},
			},
			{
			},
		},
		["guid"] = "66AD31B24035FB93B6791AB5EE41A944",
		["guid_str"] = "4vRbvHM7",
		["id"] = 31,
		["name"] = "巫炤",
		["text"] = "不被安斯朵拉认可，冠冕不能点燃太阳。",
	},
	{
