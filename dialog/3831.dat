		["caption_info"] = {
			["b_use_mask"] = true,
			["height"] = 200,
			["mask_alpha"] = 0.69999998807907,
			["text"] = "[size 17][indent][indent]我站在档案室，对着戴医生的资料陷入沉思。",
			["tween"] = {
				{
					["param"] = {
						["delay"] = 0,
