		},
		["caption_info"] = {
			["b_use_mask"] = true,
			["height"] = 200,
			["mask_alpha"] = 0.69999998807907,
			["text"] = "[size 17][indent][indent]我又在搜索框输入另一个名字。",
			["tween"] = {
				{
					["param"] = {
						["delay"] = 0,
						["time"] = 500,
