			["b_use_mask"] = true,
			["height"] = 200,
			["is_append"] = true,
			["mask_alpha"] = 0.60000002384186,
			["text"] = "\n\n[size 16]「客人A」可惜要命，大家这命从行尸手底下捡回来不容易，非要付在这种事儿上。",
		},
		["extra_sound"] = {
			["event"] = "voice_ch_quest_story_episode5025_mYGD3n8Y",
		},
		["guid"] = "5F843E7A494BA2F7EC632B9ECA5251C4",
		["guid_str"] = "mYGD3n8Y",
